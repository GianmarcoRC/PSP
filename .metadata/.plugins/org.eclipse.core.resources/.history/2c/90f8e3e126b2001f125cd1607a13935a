package Cosas;

public class PlayPass {
	  public static boolean isPrime(int num) {
		   
		  if(num <=1) {
			  return false;
		  }
		 for (int i = 0; i <= num; i++) {
			 
			 if(num / i == num || num/ i == 1 || num % i == 0) {
				return true;
			}else
				return false;
		}
		    
		    
		    return false; 
		  }
}