package Cosas;

public class PlayPass {
	 public static String[] towerBuilder(int nFloors){
		    
		    StringBuilder torre = new StringBuilder();
		    
		    for (int i = 0; i < nFloors; i++) {
				for (int j = 0; j < nFloors; j++) {
					torre.append("*");
				}
			}
		    
		    return new String[0];
		  }
	 public static void main(String[] args) {
		towerBuilder(7);
	}
}